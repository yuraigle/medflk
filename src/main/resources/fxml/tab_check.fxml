<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="ru.orlov.medflk.controller.CheckTabController"
      minHeight="0.0" minWidth="0.0" spacing="6.0"
>
    <Button fx:id="btnSelectFile" mnemonicParsing="false" onAction="#selectFile" text="Выберите файл"/>
    <TextArea fx:id="vLog" VBox.vgrow="ALWAYS">
        <VBox.margin>
            <Insets/>
        </VBox.margin>
    </TextArea>
    <padding>
        <Insets bottom="6.0" left="6.0" right="6.0" top="10.0"/>
    </padding>
</VBox>
